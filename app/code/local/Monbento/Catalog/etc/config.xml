<?xml version="1.0"?>
<config>
    <modules>
        <Monbento_Catalog>
            <version>0.1.0</version>
        </Monbento_Catalog>
    </modules>
    <global>
        <blocks>
		  <catalog>
                <rewrite>				
					<product_featured>Monbento_Catalog_Block_Product_Featured</product_featured>
					<product_new>Monbento_Catalog_Block_Product_New</product_new>
					<product_footer>Monbento_Catalog_Block_Product_Footer</product_footer>		
					<navigation>Monbento_Catalog_Block_Navigation</navigation>			
                </rewrite>
		  </catalog>
		  <page>
                <rewrite>
					<template_navigation>Monbento_Page_Block_Template_Navigation</template_navigation>
                </rewrite>
		  </page>
		  <checkout>
                <rewrite>
					<links>Monbento_Checkout_Block_Links</links>		
                </rewrite>
		  </checkout>
        </blocks>
        <resources>
            <mcatalog_setup>
                <setup>
                    <module>Monbento_Catalog</module>
                    <class>Mage_Catalog_Model_Resource_Eav_Mysql4_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </mcatalog_setup>
        </resources>
    </global>
</config>