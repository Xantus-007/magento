<?xml version="1.0" encoding="UTF-8"?><config>    <sections>        <ordersync_settings translate="label" module="core">            <label>Order Sync Settings</label>            <tab>service</tab>            <frontend_type>text</frontend_type>            <sort_order>200</sort_order>            <show_in_default>1</show_in_default>            <show_in_website>1</show_in_website>            <show_in_store>1</show_in_store>            <groups>                <general translate="label">                    <label>General</label>                    <frontend_type>text</frontend_type>                    <sort_order>10</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>1</show_in_store>                    <fields>                        <heading_export translate="label">                            <label>Export</label>                            <frontend_model>adminhtml/system_config_form_field_heading</frontend_model>                            <sort_order>05</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </heading_export>                        <allowexport translate="label">                            <label>Send Orders to Warehouse</label>                            <frontend_type>select</frontend_type>                            <comment>Enable order sync</comment>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <frontend_model>ordersync/system_config_allowexport</frontend_model>                            <sort_order>10</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </allowexport>                        <exportskus translate="label">                            <label>Send All SKUs by default</label>                            <frontend_type>select</frontend_type>                            <comment>You can override this option for each sku individually</comment>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>20</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </exportskus>                        <exportdisabled translate="label">                            <label>Send Disabled SKUs by default</label>                            <frontend_type>select</frontend_type>                            <comment>You can override this option for each sku individually</comment>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>30</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </exportdisabled>                        <exportfrom translate="label">                            <label>Send Orders from Date</label>                            <frontend_type>text</frontend_type>                            <frontend_model>ordersync/system_config_date</frontend_model>                            <comment>Leave blank to send all orders</comment>                            <sort_order>40</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </exportfrom>                        <heading_import translate="label">                            <label>Import</label>                            <frontend_model>adminhtml/system_config_form_field_heading</frontend_model>                            <sort_order>45</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </heading_import>                        <allowimport translate="label">                            <label>Get Shipments from Warehouse</label>                            <frontend_type>select</frontend_type>                            <comment>Enable shipment sync</comment>                            <frontend_model>ordersync/system_config_allowimport</frontend_model>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>50</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </allowimport>                        <sendemail translate="label">                            <label>Send Shipment Emails</label>                            <frontend_type>select</frontend_type>                            <comment>Send emails to customers once shipment received</comment>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>60</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </sendemail>                        <heading_stock translate="label">                            <label>Inventory</label>                            <frontend_model>adminhtml/system_config_form_field_heading</frontend_model>                            <sort_order>65</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </heading_stock>                        <allowstockupdate translate="label">                            <label>Get Inventory Qty from Warehouse</label>                            <frontend_type>select</frontend_type>                            <comment>Enable inventory sync</comment>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <frontend_model>ordersync/system_config_allowstockupdate</frontend_model>                            <sort_order>70</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </allowstockupdate>                        <updateskus translate="label">                            <label>Update All SKUs by default</label>                            <frontend_type>select</frontend_type>                            <comment>You can override this option for each sku individually</comment>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>80</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </updateskus>                        <updatedisabled translate="label">                            <label>Update Stock for Disabled SKUs by default</label>                            <frontend_type>select</frontend_type>                            <comment>You can override this option for each sku individually</comment>                            <source_model>adminhtml/system_config_source_yesno</source_model>                            <sort_order>90</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </updatedisabled>                    </fields>                </general>                <auth translate="label">                    <label>Credentials</label>                    <frontend_type>text</frontend_type>                    <sort_order>20</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>1</show_in_store>                    <fields>                        <key translate="label">                            <label>Key</label>                            <frontend_type>text</frontend_type>                            <sort_order>40</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </key>                        <login translate="label">                            <label>Login</label>                            <frontend_type>text</frontend_type>                            <sort_order>50</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </login>                        <password translate="label">                            <label>Password</label>                            <frontend_type>password</frontend_type>                            <sort_order>60</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </password>                        <custid translate="label">                            <label>Customer ID</label>                            <frontend_type>text</frontend_type>                            <sort_order>70</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </custid>                        <threeplid translate="label">                            <label>Three PLID</label>                            <frontend_type>text</frontend_type>                            <sort_order>80</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </threeplid>                        <fascilityid translate="label">                            <label>Facility ID</label>                            <frontend_type>text</frontend_type>                            <sort_order>90</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </fascilityid>                    </fields>                </auth>                <shipping translate="label">                    <label>Shipping Details</label>                    <frontend_type>text</frontend_type>                    <sort_order>30</sort_order>                    <show_in_default>1</show_in_default>                    <show_in_website>1</show_in_website>                    <show_in_store>1</show_in_store>                    <fields>                        <freemethod translate="label">                            <label>Free Shipping Method</label>                            <comment>for example: FedEx - Ground, USPS - Parcel Post</comment>                            <frontend_type>text</frontend_type>                            <sort_order>100</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </freemethod>                        <flatmethod translate="label">                            <label>Flat Rate Method</label>                            <comment>for example: FedEx - Ground, USPS - Parcel Post</comment>                            <frontend_type>text</frontend_type>                            <sort_order>110</sort_order>                            <show_in_default>1</show_in_default>                            <show_in_website>1</show_in_website>                            <show_in_store>1</show_in_store>                        </flatmethod>                    </fields>                </shipping>            </groups>        </ordersync_settings>    </sections></config>