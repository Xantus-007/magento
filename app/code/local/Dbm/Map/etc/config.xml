<?xml version="1.0"?>
<config>
    <modules>
        <Dbm_Map>
            <version>0.0.1</version>
        </Dbm_Map>
    </modules>
    <global>
        <helpers>
            <dbm_map>
                <class>Dbm_Map_Helper</class>
            </dbm_map>
        </helpers>
        <blocks>
            <dbm_map>
                <class>Dbm_Map_Block</class>
            </dbm_map>
        </blocks>
        <models>
            <dbm_map>
                <class>Dbm_Map_Model</class>
            </dbm_map>
        </models>
    </global>
    <frontend>
        <routers>
            <dbm_map_front>
                <use>standard</use>
                <args>
                    <module>Dbm_Map</module>
                    <frontName>club-map</frontName>
                </args>
                    
            </dbm_map_front>
        </routers>
        <layout>
            <updates>
                <dbm_map module="Dbm_Map">
                    <file>dbm/map.xml</file>
                </dbm_map>
            </updates>
        </layout>
    </frontend>
</config>